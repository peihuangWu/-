page {
  background-image: url(data:image/jpg;base64,);
}

.messageBar {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 40rpx;
  font-weight: bold;
  background-image: linear-gradient(#87CEEB, #00BFFF);
  text-align: center;
  justify-content: center;
}

.message {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  border-bottom: #708090 solid 1rpx;
  position: relative;
}

.message text{
  margin-left: 20rpx;
  margin-right: 50rpx;
}

.message image {
  margin-top: 10rpx;
  width: 60rpx;
  height: 60rpx;
}

.message image:last-of-type {
  position: absolute;
  right: 20rpx;
}