.info {
  width: 100%;
  background-image: url(data:image/jpg;base64,);
}

.info .view {
  display: flex;
  min-height: 80rpx;
  line-height: 80rpx;
}

.info .view image {
  width: 60rpx;
  height: 60rpx;
  margin-top: 10rpx;
}

.info .view text {
    margin-left: 30rpx;
}

.signature {
  display: flex;
  flex-direction: column;
}

.signature > text {
  padding-left: 100rpx;
}

.signature .view{
  display: flex;
}

.goodsList{
  /* margin-top: 30rpx; */
  width: 100%;
}

.goodsList .goodsListNav{
  width: 100%;
  height: 50rpx;
  background-color:#E8E8E8;
  font-size: 30rpx;
  line-height: 50rpx;
}

.goodsList .goodsListNav text {
  float: left;
  margin-left: 20rpx;
  font-weight: bold;
}

.goods{
  margin: 10rpx;
  border-top: #B8B8B8 solid 1px;
  width: 95%;
  height: 120px;
  position: relative;
  display: flex;
  justify-content: center;
  flex-direction: row;
}

.goods .goodsImg {
  width: 400rpx;
  height: 250rpx;
}

.goods .goodsImg image {
  height: 100%;
  width: 100%;
}

.goods .goordsInfo {
  margin-left: 20rpx;
  position: relative;
}

.goods .goordsInfo .goodsTitle{
  font-size: 25rpx;
}

.goods .goordsInfo .goodsPrice{
  font-size: 30rpx;
  position: absolute;
  bottom: 30rpx;
  right: 50rpx;
}

.goods .goordsInfo .goodsPrice text{
  color: red;
}
.goods .goordsInfo .sellNum {
  font-size: 30rpx;
  position: absolute;
  bottom: 30rpx;
  right: 300rpx;
}
